!! scrollback buffer lines - 65535 is max on most machines (64 is default)
! URxvt.saveLines: 12000

URxvt.foreground: #BBB
URxvt.background: #040404

! Xft.hintstyle: hintslight
! Xft.rgba: rgb
URxvt.letterSpace: -1

!! I would rather use Source Code Pro, but it lacks certain Unicode glyphs 
!! and I have issues getting font list fallbacks working...
URxvt.font: xft:Droid Sans Mono Slashed:regular:pixelsize=12:antialias=true:hinting=true,xft:Symbola
URxvt.boldFont: xft:Droid Sans Mono Slashed:bold:pixelsize=12:antialias=true:hinting=true,xft:Symbola
!URxvt.font: xft:Source Code Pro:regular:pixelsize=12:antialias=true:hinting=true, \
!            xft:Droid Sans Mono:regular:pixelsize=12:antialias=true:hinting=true
!URxvt.boldFont: xft:Source Code Pro:bold:pixelsize=12:antialias=true:hinting=true, \
!                xft:Droid Sans Mono:bold:pixelsize=12:antialias=true:hinting=true
!URxvt.italicFont: 
!URxvt.bolditalicFont: 

! black
URxvt.color0  : #2E3436
URxvt.color8  : #555753
! red
URxvt.color1  : #CC0000
URxvt.color9  : #EF2929
! green
URxvt.color2  : #4E9A06
URxvt.color10 : #8AE234
! yellow
URxvt.color3  : #C4A000
URxvt.color11 : #FCE94F
! blue
URxvt.color4  : #3465A4
URxvt.color12 : #729FCF
! magenta
URxvt.color5  : #75507B
URxvt.color13 : #AD7FA8
! cyan
URxvt.color6  : #06989A
URxvt.color14 : #34E2E2
! white
URxvt.color7  : #D3D7CF
URxvt.color15 : #EEEEEC


URxvt.scrollBar: false 
URxvt.cursorBlink: true 

